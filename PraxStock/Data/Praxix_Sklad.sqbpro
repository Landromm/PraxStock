id_" freeze_columns="0"><sort/><column_widths><column index="1" value="46"/><column index="2" value="67"/><column index="3" value="78"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="CREATE Table Praxix_Sklad">CREATE TABLE Items
(
    idItem INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    NameItem TEXT NOT NULL,
	UnitMeasure TEXT NOT NULL
);

CREATE TABLE MoveInPost
(
	idMove INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
	idItems INTEGER NOT NULL,
	QuantityMove REAL NOT NULL,
	NamePost TEXT,
	DateMove TEXT,
	idItemStock INTEGER NOT NULL,
	
    CONSTRAINT MoveInPost_Items_fk 
    FOREIGN KEY (idItems)  REFERENCES Items (idItem),
	CONSTRAINT MoveInPost_DataStock_fk
	FOREIGN KEY (idItemStock) REFERENCES DataStock (idItemStock)
);

CREATE TABLE WriteOff
(
	idWriteOff INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
	idItem INTEGER NOT NULL,
	QuantityWriteOff REAL NOT NULL,
	DateWriteOff TEXT NOT NULL,
	
	CONSTRAINT WriteOff_Items_fk
	FOREIGN KEY (idItem) REFERENCES Items (idItem)
);

CREATE TABLE DataStock
(
	idItemStock INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
	idItem INTEGER NOT NULL,
	RemainingStock REAL NOT NULL,
	MinValue REAL,
	FlagSett BLOB,
	
	CONSTRAINT DataStock_Items_fk
	FOREIGN KEY (idItem) REFERENCES Items (idItem)
);

CREATE TABLE Receipt
(
	idReceipt INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
	idItem INTEGER NOT NULL,
	QuantityReceipt REAL NOT NULL,
	ExpirationDate TEXT,
	DateReceipt TEXT NOT NULL,
	
	CONSTRAINT Receipt_Items_fk
	FOREIGN KEY (idItem) REFERENCES Items (idItem),
	CONSTRAINT Receipt_DataStock_fk
	FOREIGN KEY (idReceipt) REFERENCES DataStock (idItemStock)
);</sql><sql name="OTHER QUERY*">SELECT 
DataStock.idItemStock, DataStock.idItem, Items.NameItem,
Items.UnitMeasure, DataStock.RemainingStock,
Receipt.ExpirationDate, Receipt.DateReceipt,
DataStock.MinValue, DataStock.FlagSett 
FROM DataStock
INNER  JOIN Items ON DataStock.idItem = Items.idItem
INNER JOIN Receipt ON DataStock.idItemStock = Receipt.idReceipt
ORDER BY Items.NameItem

INSERT INTO Receipt (idItem, QuantityReceipt, ExpirationDate, DateReceipt)
VALUES (1, 22, NULL, '15-04-2025')</sql><current_tab id="1"/></tab_sql></sqlb_project>
